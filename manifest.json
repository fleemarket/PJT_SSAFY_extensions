{
    "manifest_version": 2,
  
    "name": "SSAFY_extensions",
    "description": "Print Hello, World! message",
    "version": "1.0.0",
  
    "browser_action": {
      "default_icon": "ssafy-logo.png",
      "default_popup": "dpopup.html"
    },
    "permissions": [
      "tabs",
      "<all_urls>"
    ],
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["/my-content-script.js"],
      "run_at": "document_start"
    }],
    "background": {
      "scripts": ["/my-background-script.js"],
      "persistent": false
    }
  }