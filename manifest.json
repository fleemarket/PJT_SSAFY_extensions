{
    "manifest_version": 2,
  
    "name": "JONGTO_cleaner",
    "short_name" : "JT_cleaner",
    "description": "more comfortable stock-discussion board",
    "version": "1.0.2",
    "author" : "fleemarket",
  
    "browser_action": {
      "default_icon": "jongto-logo.png",
      "default_popup": "dpopup.html"
    },
    "permissions": [
      "tabs",
      "<all_urls>"
    ],
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["/my-content-script.js"],
      "run_at": "document_start"
    }],
    "background": {
      "scripts": ["/my-background-script.js"],
      "persistent": false
    }
  }